//packetIDConstants -1 -- -128: reserved for error packets.
-128=OutOfSyncPacket
//packetIDConstants 0: assigned to Abstract Base Packet should never be sent might send an exception at the offending client.
0=Packet
//packetIDConstants 1 -- 63: reserved for connection packets should not be
//used after packets have been sent.
1=ConnectPacket
2=ConnectionAcceptedPacket
3=ConnectionDeniedPacket
//packetIDConstants 64 -- 127: used for requesting and receiving die rolls.
64=SendNewPlayerPacket
65=RollDicePacket
66=ResultPacket
67=MultiResultPacket
68=MultiResultEndPacket
69=ChatPacket
70=KickPacket

